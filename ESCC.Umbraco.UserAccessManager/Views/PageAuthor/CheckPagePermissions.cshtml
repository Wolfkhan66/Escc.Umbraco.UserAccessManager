@model  IEnumerable<ESCC.Umbraco.UserAccessManager.Models.PermissionsModel>

<h4>People responsible for the page:</h4>
<h4>@Model.First().PageName</h4>

<ul class="list-group-x">
    @foreach (var item in @Model)
    {
        <li class="list-group-item-x">
            <p>
                @item.FullName
                -
                @item.EmailAddress
            </p>
        </li>
    }
</ul>

@{
    Html.RenderAction("Index", "WebEditors");   
}
