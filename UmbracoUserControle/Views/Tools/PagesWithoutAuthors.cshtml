@model IEnumerable<UmbracoUserControl.Models.PermissionsModel>

    <h4>Pages without web authors</h4>

    <ul class="list-group">
        @foreach (var page in @Model)
        {
            <li class="list-group-item">
                <ul>
                    <li>
                        <p>@page.PageId</p>
                    </li>
                    <li>
                        <p>@page.PageName</p>
                    </li>
                </ul>
            </li>
        }
    </ul>